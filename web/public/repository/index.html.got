<style>
    .demo-list-item {
        width: 300px;
    }
</style>

<span>Master : {{.masterRepo}}</span>
<br/>
<span>Encrypt : {{.encryptRepo}}</span>

<ul class="demo-list-item mdl-list">
    {{ range $i,$f := .files }}
    <li class="mdl-list__item">
        {{$i}}.
        <span class="mdl-list__item-primary-content">
        {{$f.Name}}
        {{$f.Path}}
        </span>
    </li>
    {{ end }}
</ul>